	"version": "0.2.0",
	"configurations": [
		{
			"type": "chrome",
			"request": "launch",
			"name": "Launch Chrome against development server",
			"url": "http://localhost:3000",
			"webRoot": "${workspaceFolder}",
			"preLaunchTask": "npm: start-dev-server",
		},
		{
			"name": "Launch Firefox against development server",
			"type": "firefox",
			"request": "launch",
			"url": "http://localhost:3000",
			"webRoot": "${workspaceFolder}",
			"preLaunchTask": "npm: start-dev-server",
		}
	]
}